{
	"name": "loadenv-cli",
	"version": "0.3.1",
	"description": "Use Vite's `loadEnv` on the command-line",
	"license": "MIT",
	"author": "Jan T. Sott",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/idleberg/loadenv-cli.git"
	},
	"keywords": [
		"environment variables",
		"dotenv",
		"load environment",
		"vite"
	],
	"type": "module",
	"bin": {
		"loadenv": "./bin/index.js"
	},
	"engines": {
		"node": ">=22"
	},
	"files": [
		"bin/",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"lint": "concurrently --prefix-colors blue,green,magenta npm:lint:*",
		"lint:biome": "biome check",
		"lint:e18e": "e18e-cli analyze",
		"prepack": "npm run build",
		"prepare": "lefthook install",
		"publish:jsr": "deno publish",
		"publish:npm": "np --any-branch",
		"test": "vitest run"
	},
	"dependencies": {
		"commander": "^14.0.2",
		"kleur": "^4.1.5",
		"vite": "^7.1.12"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@e18e/cli": "^0.3.1",
		"@idleberg/configs": "^0.3.1",
		"@types/node": "^24.9.2",
		"@vitest/coverage-v8": "^4.0.5",
		"concurrently": "^9.2.1",
		"esbuild": "^0.25.11",
		"np": "^10.2.0",
		"tsdown": "^0.15.12",
		"vitest": "^4.0.5"
	},
	"packageManager": "pnpm@10.18.1+sha512.77a884a165cbba2d8d1c19e3b4880eee6d2fcabd0d879121e282196b80042351d5eb3ca0935fa599da1dc51265cc68816ad2bddd2a2de5ea9fdf92adbec7cd34"
}
