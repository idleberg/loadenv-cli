{
	"name": "loadenv-cli",
	"version": "0.3.1",
	"description": "Use Vite's `loadEnv` on the command-line",
	"license": "MIT",
	"author": "Jan T. Sott",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/idleberg/loadenv-cli.git"
	},
	"keywords": [
		"environment variables",
		"dotenv",
		"load environment",
		"vite"
	],
	"type": "module",
	"bin": {
		"loadenv": "./bin/index.js"
	},
	"engines": {
		"node": ">=22"
	},
	"files": [
		"bin/",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"lint": "concurrently --prefix-colors blue,green,magenta npm:lint:*",
		"lint:biome": "biome check",
		"lint:e18e": "e18e-cli analyze",
		"prepack": "npm run build",
		"prepare": "lefthook install",
		"publish:jsr": "deno publish",
		"publish:npm": "np --any-branch",
		"test": "vitest run"
	},
	"dependencies": {
		"commander": "^14.0.2",
		"kleur": "^4.1.5",
		"vite": "^7.2.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@e18e/cli": "^0.3.1",
		"@idleberg/configs": "^0.3.3",
		"@types/node": "^24.10.1",
		"@vitest/coverage-v8": "^4.0.9",
		"concurrently": "^9.2.1",
		"np": "^10.2.0",
		"tsdown": "^0.16.4",
		"vitest": "^4.0.9"
	},
	"packageManager": "pnpm@10.21.0+sha512.da3337267e400fdd3d479a6c68079ac6db01d8ca4f67572083e722775a796788a7a9956613749e000fac20d424b594f7a791a5f4e2e13581c5ef947f26968a40"
}
